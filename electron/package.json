{
  "name": "electron",
  "private": true,
  "version": "0.0.1",
  "productName": "signal",
  "main": "dist/index.js",
  "scripts": {
    "dev": "concurrently \"tsc -w\" \"tsc -p tsconfig.preload.json -w\" \"nodemon\"",
    "start": "npm run build && electron .",
    "build": "tsc && tsc -p tsconfig.preload.json",
    "package": "npm run build && rm -rf out && electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "generate-icon": "./scripts/icons.sh src/icons/Icon-macOS.png ./icons/icon.icns"
  },
  "author": "ryohey",
  "devDependencies": {
    "@electron-forge/cli": "7.3.0",
    "@electron-forge/maker-pkg": "^7.4.0",
    "concurrently": "^8.2.2",
    "electron": "^29.3.0",
    "nodemon": "^3.1.0",
    "typescript": "5.4.2"
  },
  "dependencies": {
    "express": "^4.19.2",
    "get-port-please": "^3.1.2"
  }
}
